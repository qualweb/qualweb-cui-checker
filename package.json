{
  "name": "chatbot-extension",
  "version": "1.0.0",
  "description": "An evaluation tool to check the accessibility of the current chatbot interface.",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "npm run prettier -- --write",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch",
    "dev": "NODE_ENV=development webpack --watch"
  },
  "dependencies": {
    "@qualweb/act-rules": "^0.0.0-develop-20241204125016",
    "@qualweb/best-practices": "^0.6.19",
    "@qualweb/cui-checks": "^0.1.2",
    "@qualweb/locale": "0.1.18",
    "@qualweb/qw-page": "0.2.19",
    "@qualweb/util": "0.5.37",
    "@qualweb/wcag-techniques": "0.3.17",
    "axios": "^1.7.7",
    "file-saver": "^2.0.2",
    "highlight.js": "^9.18.1",
    "htmlfy": "^0.5.0",
    "shadow-dom-selector": "^5.0.1",
    "vue": "^2.6.10",
    "vue-code-highlight": "^0.7.4",
    "vue-highlight.js": "^3.1.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime-corejs3": "^7.4.0",
    "@types/chrome": "^0.0.74",
    "archiver": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.4.0",
    "deepmerge": "^4.2.2",
    "ejs": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "^5.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "sass": "^1.26.2",
    "sass-loader": "^10.0.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^8.0.0",
    "typescript": "^4.0.0",
    "vue-loader": "^15.11.1",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.10",
    "webpack-extension-reloader": "^1.1.0"
  }
